{
  "name": "quickstart.vue3-web-template",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier . --write",
    "commit": "git-cz",
    "prepare": "husky install",
    "update:request":"npx http-request-cli -g"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@gopowerteam/http-request": "0.3.0",
    "@vueuse/core": "6.0.0",
    "@vueuse/head": "0.6.0",
    "ant-design-vue": "2.2.6",
    "lodash-es": "4.17.21",
    "nprogress": "0.2.0",
    "pinia": "2.0.0-rc.6",
    "ramda": "0.27.1",
    "vue": "3.2.6",
    "vue-router": "4.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@gopowerteam/http-request-cli": "0.3.0",
    "@iconify/json": "1.1.394",
    "@prettier/plugin-pug": "1.16.5",
    "@types/lodash-es": "4.17.4",
    "@types/nprogress": "0.2.0",
    "@types/ramda": "0.27.44",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "@vitejs/plugin-vue": "1.6.0",
    "@vitejs/plugin-vue-jsx": "1.1.7",
    "@vue/compiler-sfc": "3.2.6",
    "@vue/server-renderer": "3.2.6",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "7.17.0",
    "husky": "7.0.2",
    "less": "4.1.1",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "prettier-plugin-packagejson": "2.2.11",
    "typescript": "4.3.5",
    "unplugin-auto-import": "^0.2.4",
    "unplugin-icons": "0.7.2",
    "vite": "2.5.1",
    "vite-plugin-components": "0.13.2",
    "vite-plugin-eslint": "1.3.0",
    "vite-plugin-pages": "0.17.5",
    "vite-plugin-pwa": "0.11.2",
    "vite-plugin-vue-layouts": "0.3.1",
    "vite-plugin-windicss": "1.3.0",
    "vite-ssg": "0.14.7",
    "vite-svg-loader": "2.2.0",
    "vue-tsc": "0.3.0",
    "windicss": "3.1.7"
  },
  "engines": {
    "node": ">=14"
  }
}
